<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans">


    <bean id="person" class="org.springframework.beans.factory.test.ioc.bean.Person">
        <property name="name" value="derek"/>
        <property name="car" ref="car"/>
    </bean>

    <bean id="car" class="org.springframework.beans.factory.test.ioc.bean.Car">
        <property name="brand" value="porsche"/>
    </bean>


    <bean class="org.springframework.beans.factory.test.ioc.common.CustomBeanPostProcessor"/>
    <bean class="org.springframework.beans.factory.test.ioc.common.CustomBeanFactoryPostProcessor"/>
    <bean id = "helloService" class="org.springframework.beans.factory.test.ioc.service.HelloService"/>
</beans>